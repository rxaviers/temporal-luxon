{
  "name": "temporal-luxon",
  "version": "0.0.3",
  "description": "Temporal API using luxon under the hoods",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "LICENSE-MIT",
    "README.md",
    "src/",
    "dist/"
  ],
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./src/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rxaviers/temporal-luxon.git"
  },
  "keywords": [
    "temporal",
    "zoneddatetime",
    "plaindate",
    "plaintime",
    "plaindatetime",
    "date",
    "time",
    "datetime",
    "timezone",
    "zone",
    "metazone",
    "iana"
  ],
  "author": {
    "name": "Rafael Xavier de Souza",
    "email": "rxaviers@gmail.com",
    "url": "http://rafael.xavier.blog.br"
  },
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rxaviers/temporal-luxon/issues"
  },
  "homepage": "https://github.com/rxaviers/temporal-luxon#readme",
  "scripts": {
    "test": "eslint --ext .js --ext .mjs src && mocha",
    "build": "tsup --format cjs --format esm src/index.mjs"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "eslint": "^8.23.1",
    "eslint-plugin-mocha": "^10.1.0",
    "mocha": "^10.0.0",
    "tsup": "^6.2.3"
  },
  "dependencies": {
    "luxon": "^3.0.3"
  }
}
